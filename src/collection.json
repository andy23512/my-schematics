{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "component-store": {
      "description": "Generate Component Store",
      "factory": "./component-store/index#genComponentStore",
      "schema": "./component-store/schema.json"
    },
    "component": {
      "description": "Generate Component",
      "factory": "./component/index#genComponent",
      "schema": "./component/schema.json"
    },
    "directive": {
      "description": "Generate Directive",
      "factory": "./directive/index#genDirective",
      "schema": "./directive/schema.json"
    }
  }
}
